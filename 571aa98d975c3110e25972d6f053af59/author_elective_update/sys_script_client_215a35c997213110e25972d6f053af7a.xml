    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>get_data_from_cmdb</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Get RU count form CMDB</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }
    var formVal = g_form.getValue('resource_unit_number');
    var ga = new GlideAjax('currentRuCount');
    ga.addParam('sysparm_name', 'getCmdbCount');
    ga.addParam('sysparm_ru_name', formVal);
    ga.getXML(countParse);

    function countParse(response) {

        var answer = response.responseXML.documentElement.getAttribute("answer");

        var clearvalue;

		if(newValue == 'yes'){
			g_form.addInfoMessage(answer);
			//g_form.setValue()
			g_form.setValue("current_month_count", answer.count.toSting());
			g_form.setValue("u_ru_description", answer.description);
			g_form.setValue("u_arc_rrc_rate", answer.arcrate);
			g_form.setValue("u_baseline", answer.baseline);
		}
		else if(newValue == 'no'){
			g_form.clearValue("current_month_count");
		}
		else{
			g_form.clearValue("current_month_count");
		}
    }


}]]></script>
